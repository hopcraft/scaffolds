  "description": "Vue.js 移动端脚手架",
  "scripts": {
    "start": "cross-env NODE_ENV=local webpack-dev-server --inline --hot --no-info --port 2001",
    "dist": "npm run dist-dev & npm run dist-prod",
    "dist-dev": "rimraf dist/dev && cross-env NODE_ENV=development webpack --progress --hide-modules",
    "dist-prod": "rimraf dist/prod && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "mock": "nodemon ./mocks/"
  },
  "keywords": [
    "移动端",
    "Vue.js",
    "脚手架"
  ],
